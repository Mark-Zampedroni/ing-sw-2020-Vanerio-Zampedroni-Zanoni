<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="main" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8">
    <bottom>
        <Button fx:id="okButton" mnemonicParsing="false" prefHeight="35.0" prefWidth="52.0" text="OK"
                textAlignment="CENTER" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="20.0"/>
            </BorderPane.margin>
        </Button>
    </bottom>
    <top>
        <Label fx:id="dialogTitle" alignment="TOP_CENTER" contentDisplay="CENTER" prefHeight="51.0" prefWidth="600.0"
               text="Labe" BorderPane.alignment="CENTER">
            <font>
                <Font size="30.0"/>
            </font>
        </Label>
    </top>
    <center>
        <Label fx:id="dialogText" prefHeight="154.0" prefWidth="426.0" text="Label" wrapText="true"
               BorderPane.alignment="CENTER">
            <font>
                <Font size="25.0"/>
            </font>
        </Label>
    </center>
</BorderPane>
